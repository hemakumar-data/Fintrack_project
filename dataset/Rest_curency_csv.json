{
	"name": "Rest_curency_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"p_date": {
				"type": "string"
			},
			"p_from": {
				"type": "string"
			},
			"p_to": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "from=@{dataset().p_from}_to=@{dataset().p_to}.csv\n\n",
					"type": "Expression"
				},
				"folderPath": {
					"value": "currency/date=@{dataset().p_date}\n",
					"type": "Expression"
				},
				"fileSystem": "bronze"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	}
}